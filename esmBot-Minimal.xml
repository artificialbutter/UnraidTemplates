<?xml version="1.0"?>
<Container version="2">
  <Name>esmBot-Minimal</Name>
  <Repository>weilbyte/esmbotmin</Repository>
  <Registry>https://hub.docker.com/r/weilbyte/esmbotmin/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://hub.docker.com/r/weilbyte/esmbotmin/</Support>
  <Project/>
  <Overview>A stateless fork of esmBot with only image manipulation functions, no API and no database.&#xD;
NOTE: Tenor API in use is discontinued. Tenor GIFs will not be processed.&#xD;
&#xD;
NOT RECOMMENDED FOR PRODUCTION USE.</Overview>
  <Category>Other:</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://esmbot.net/pictures/esmbot.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1683188984</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Log Location" Target="/logs" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/esmbot-min/logs</Config>
  <Config Name="Discord Token" Target="TOKEN" Default="" Mode="" Description="Insert the token for the bot from https://discord.com/developers/applications" Type="Variable" Display="always" Required="true" Mask="true"></Config>
  <Config Name="Owner ID" Target="OWNER" Default="" Mode="" Description="Put discord user ID of bot owner here." Type="Variable" Display="always" Required="true" Mask="false"></Config>
  <Config Name="Bot Prefix" Target="PREFIX" Default="&amp;amp;" Mode="" Description="Prefix for the bot to respond to." Type="Variable" Display="always" Required="true" Mask="false">&amp;</Config>
  <Config Name="Processing Emoji" Target="PROCESSING_EMOJI" Default=":gear:" Mode="" Description="Put the emoji for processing messages here" Type="Variable" Display="always" Required="false" Mask="false">:gear:</Config>
  <Config Name="Node Version" Target="NODE_VERSION" Default="15.13.0" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">15.13.0</Config>
  <Config Name="Yarn Version" Target="YARN_VERSION" Default="1.22.5" Mode="" Description="" Type="Variable" Display="advanced" Required="false" Mask="false">1.22.5</Config>
</Container>